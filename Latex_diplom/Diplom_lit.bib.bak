title = {{On the integration of in silico drug design methods for drug repurposing}},
volume = {8},
year = {2017}
}

@Article{Ehrt2016,
author={Ehrt, Christiane
and Brinkjost, Tobias
and Koch, Oliver},
title={Impact of Binding Site Comparisons on Medicinal Chemistry and Rational Molecular Design},
journal={Journal of Medicinal Chemistry},
year={2016},
